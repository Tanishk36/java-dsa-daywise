class Solution {
    public int[] constructTransformedArray(int[] nums) {

        int n=nums.length;
        int[] ans=new int[n];

        for(int i=0;i<n;i++){
            int k=(i+nums[i])%n;


            if(k<0){
                k+=n;
            }
            ans[i]=nums[k];
        }
        return ans;
        
    }
}
