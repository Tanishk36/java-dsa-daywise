class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int count1=0;
        int maxcount=0;

        for(int num:nums){
            if(num==1){
                count1++;
                if(count1>maxcount){
                    maxcount=count1;
                }
            }else{
                count1=0;
            }
        }
        return maxcount;
    }


}
